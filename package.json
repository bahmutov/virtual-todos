{
  "name": "virtual-todos",
  "description": "TodoMVC app rendering function implemented using virtual-dom",
  "main": "src/render.js",
  "version": "0.0.0-semantic-release",
  "scripts": {
    "test": "rocha src/*-spec.js",
    "lint": "standard *.js src/*.js",
    "commit": "commit-wizard",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/virtual-todos.git"
  },
  "keywords": [
    "todo",
    "todos",
    "todomvc",
    "virtual-dom"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/virtual-todos/issues"
  },
  "homepage": "https://github.com/bahmutov/virtual-todos#readme",
  "dependencies": {
    "todomvc-app-css": "2.0.3",
    "todomvc-common": "1.0.2",
    "virtual-dom": "2.1.1"
  },
  "devDependencies": {
    "check-more-types": "2.10.0",
    "lazy-ass": "1.3.0",
    "pre-git": "3.1.2",
    "rocha": "1.6.1",
    "standard": "5.4.1",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "pre-git": {
      "commit-msg": [
        "simple"
      ],
      "pre-commit": [
        "npm run lint",
        "npm test"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-merge": []
    }
  }
}
